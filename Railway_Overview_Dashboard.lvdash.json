    {
      "name": "67d8e8c8",
      "displayName": "Journey Status Distribution",
      "queryLines": [
        "SELECT \n",
        "`Journey Status`, \n",
        "COUNT(*) AS count,\n",
        "Round(COUNT(*)*100.0/SUM(COUNT(*)) OVER(),2) AS percentage\n",
        "FROM workspace.default.railway\n",
        "GROUP BY `Journey Status`\n",
        "ORDER BY count DESC;"
      ]
    },
