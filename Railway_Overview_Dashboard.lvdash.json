{
  "datasets": [
    {
      "name": "67d8e8c8",
      "displayName": "Payment Method Distribution",
      "queryLines": [
        "SELECT `Payment Method`, COUNT(*) AS count\n",
        "FROM workspace.default.railway\n",
        "GROUP BY `Payment Method`\n",
        "ORDER BY count DESC\n"
      ]
    },
    {
      "name": "f10be708",
      "displayName": "railway",
      "queryLines": [
        "SELECT * FROM workspace.default.railway"
      ]
    },
    {
      "name": "b24a08ec",
      "displayName": "Journey Status breakdown",
      "queryLines": [
        "SELECT \r\n",
        "    `Journey Status`, \r\n",
        "    COUNT(*) AS count,\r\n",
        "    ROUND(COUNT(*) * 100.0 / SUM(COUNT(*)) OVER(), 2) AS percentage\r\n",
        "FROM workspace.default.railway\r\n",
        "GROUP BY `Journey Status`\r\n",
        "ORDER BY count DESC;"
      ]
    },
    {
      "name": "05db5f2e",
      "displayName": "Journey Status Trend Over Time",
      "queryLines": [
        "SELECT `Date of Journey`, `Journey Status`, COUNT(*) AS count\n",
        "FROM workspace.default.railway\n",
        "GROUP BY `Date of Journey`, `Journey Status`\n",
        "ORDER BY `Date of Journey`, `Journey Status`"
      ]
    },
    {
      "name": "f17c143d",
      "displayName": "Purchase Type by Ticket Class",
      "queryLines": [
        "SELECT `Purchase Type`, `Ticket Class`, COUNT(*) AS count\n",
        "FROM workspace.default.railway\n",
        "GROUP BY `Purchase Type`, `Ticket Class`\n",
        "ORDER BY `Purchase Type`, `Ticket Class`"
      ]
    },
    {
      "name": "4f5c023b",
      "displayName": "Top 10 Most popular Routes",
      "queryLines": [
        "SELECT \r\n",
        "    `Departure Station`,\r\n",
        "    `Arrival Destination`,\r\n",
        "    CONCAT(`Departure Station`, ' â†’ ', `Arrival Destination`) AS route,\r\n",
        "    COUNT(*) AS booking_count,\r\n",
        "    AVG(Price) AS avg_price,\r\n",
        "    SUM(Price) AS total_revenue\r\n",
        "FROM workspace.default.railway\r\n",
        "GROUP BY `Departure Station`, `Arrival Destination`\r\n",
        "ORDER BY booking_count DESC\r\n",
        "LIMIT 10;"
      ]
    }
  ],
